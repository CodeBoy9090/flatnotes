	<SolidColorBrush x:Key="KeepNoteOrangeBrush" Color="#FFFFBB22"/>
	<SolidColorBrush x:Key="KeepNoteYellowBrush" Color="#FFEEEE22"/>
	<SolidColorBrush x:Key="KeepNoteGreenBrush" Color="#FFBBE535"/>
	<SolidColorBrush x:Key="KeepNoteTealBrush" Color="#FF77DDBB"/>
