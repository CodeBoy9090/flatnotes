    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Keep">

    <SolidColorBrush x:Key="KeepNoteForegroundBrush" Color="#A0000000"/>
    <SolidColorBrush x:Key="TextBoxForegroundThemeBrush" Color="#FF000000"/>
    <SolidColorBrush x:Key="TextBoxFocusedBackgroundThemeBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="KeepBrandBackgroundBrush" Color="#FFFFCD35"/>
